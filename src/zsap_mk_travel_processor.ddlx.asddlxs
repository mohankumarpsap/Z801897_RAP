  //  @UI.fieldGroup: [{ qualifier: 'admin', position: 40 }]
  //  LastChangedAt;



  @UI.fieldGroup: [{ qualifier: 'admin', position: 10 }]
  BeginDate;
  @UI.fieldGroup: [{ qualifier: 'admin', position: 20 }]
  EndDate;
  @UI.selectionField: [{ position: 50 }]
  @UI.lineItem: [{ position: 60 , criticality: 'criticality' , importance: #HIGH }]
  @UI.dataPoint: { qualifier: 'status', title: 'Status', criticality: 'criticality' }
  @UI.fieldGroup: [{ position: 30, qualifier: 'admin' }]
