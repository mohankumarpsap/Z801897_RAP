@Metadata.layer: #CORE
@UI.headerInfo:
{
  typeName: 'Travel',
  typeNamePlural: 'Travels',
  title:{ value: 'TravelId' },
  description:{ value: '_Customer.FirstName' }
}
@Search.searchable: true


//Defining Multiple Views on a List Report Table - Single Table Mode
//https://sapui5.hana.ondemand.com/sdk/#/topic/0d390fed360c4c58a0f0619338938de1.html
@UI.selectionVariant: [
{ qualifier: 'All', text: 'all' },
{ qualifier: 'Open', text: 'Open', filter: ' OverallStatus eq O ' },
{ qualifier: 'Accepted', text: 'Approved', filter: ' OverallStatus eq A ' },
{ qualifier: 'Cancelled', text: 'Cancelled', filter: ' OverallStatus eq N ' }
]

annotate entity ZSAP_MK_TRavel_processor with
{

  @UI.facet: [
  {
  purpose: #HEADER,
  label: 'Traveling Object Page',
  position: 10   },

  {
  purpose: #HEADER,
  type: #DATAPOINT_REFERENCE,
  label: 'Price',
  targetQualifier: 'travel',
  position: 20
  },


  {
  purpose: #HEADER,
  type: #DATAPOINT_REFERENCE,
  label: 'Status',
  targetQualifier: 'status',
  position: 30
  },


  {
  purpose: #STANDARD,
  type: #COLLECTION,
  label: 'Adional Info',
  position: 10,
  id : 'super'
   },

